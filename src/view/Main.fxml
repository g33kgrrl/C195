<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane id="login">
         <children>
            <TextField fx:id="userName" layoutX="136.0" layoutY="65.0" prefHeight="25.0" prefWidth="168.0" promptText="Enter username" text="foo" />
            <TextField fx:id="password" layoutX="136.0" layoutY="95.0" prefHeight="25.0" prefWidth="168.0" promptText="Enter password" text="bar" />
            <Button layoutX="284.0" layoutY="135.0" mnemonicParsing="false" onAction="#onSubmitButtonAction" text="Submit" />
            <Label layoutX="57.0" layoutY="68.0" text="Username:" />
            <Label layoutX="57.0" layoutY="98.0" text="Password:" />
            <Label layoutX="59.0" layoutY="27.0" text="Please enter your username and password.">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="zoneIdLabel" layoutX="31.0" layoutY="139.0" text="Label" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </AnchorPane>
   </children>
</GridPane>
