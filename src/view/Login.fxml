<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="controller.LoginController">
   <columnConstraints>
      <ColumnConstraints/>
   </columnConstraints>
   <rowConstraints>
      <RowConstraints/>
   </rowConstraints>
   <AnchorPane id="login">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
      <TextField fx:id="userNameText" layoutX="136.0" layoutY="65.0" prefHeight="25.0" promptText="Enter username"
                 text="test" AnchorPane.bottomAnchor="80.0" AnchorPane.rightAnchor="25.0"/>
      <TextField fx:id="passwordText" prefHeight="25.0" promptText="Enter password" text="test"
                 AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="25.0"/>
      <Button fx:id="submitButton" mnemonicParsing="false" onAction="#onSubmitButtonAction" text="Submit"
              AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0"/>
      <Label fx:id="userNameLabel" layoutX="57.0" layoutY="68.0" text="Username:" AnchorPane.bottomAnchor="84.0"
             AnchorPane.leftAnchor="25.0"/>
      <Label fx:id="passwordLabel" layoutX="57.0" layoutY="98.0" text="Password:" AnchorPane.bottomAnchor="54.0"
             AnchorPane.leftAnchor="25.0"/>
      <Label fx:id="messageLabel" text="Please enter your username and password." AnchorPane.bottomAnchor="120.0"
             AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="12.0">
         <font>
            <Font name="System Bold" size="12.0"/>
         </font>
      </Label>
      <Label fx:id="zoneIdLabel" text="Label" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="12.0"/>
   </AnchorPane>
</GridPane>
